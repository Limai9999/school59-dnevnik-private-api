{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
      "linebreak-style": "off",
      "require-jsdoc": "off",
      "max-len": "off",
      "semi-spacing": "warn",
      "camelcase": "off",
      "new-cap": "off"
    }
}